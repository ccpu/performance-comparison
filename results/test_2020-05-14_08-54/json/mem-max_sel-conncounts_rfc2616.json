        "scales": {
            "yAxes": [
                {
                    "ticks": {
                        "beginAtZero": true,
                        "stepSize": 100,
                        "fontFamily": "Roboto"
                    },
                    "scaleLabel": {
                        "display": true,
                        "labelString": "Peak memory consumed in MiB with minimum value shown (lower is better)",
